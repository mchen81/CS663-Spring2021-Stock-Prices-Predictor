   "source": [
    "import stock_helper\n",
    "import pandas as pd\n",
    "from datetime import datetime\n",
    "import numpy as np\n",
    "\n",
    "# MSFT > tesla > google > amd > ge\n",
    "dataset = stock_helper.fetch_stock_data('GOOGL', '30y')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 288,
   "id": "c9d8c422",
   "metadata": {},
   "outputs": [],
   "source": [
    "df = stock_helper.generate_indicators(dataset, \n",
    "                                      timeperiod=2, \n",
    "                                      generate_target=True, \n",
    "                                      reset_index=True)\n",
    "\n",
    "# Convert date to the n-th day from 1970-01-01\n",
    "df['date'] = df['date'].apply(lambda date64: (date64 - np.datetime64('1970-01-01T00:00:00')) / np.timedelta64(1, 'D'))"
   ]
  },
